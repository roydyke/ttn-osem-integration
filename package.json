    "lorawan",
    "sensorweb",
    "open",
    "data"
  ],
  "author": "github.com/noerw",
  "license": "GPL-3.0",
  "bugs": {
    "url": "https://github.com/sensebox/ttn-osem-integration/issues"
  },
  "homepage": "https://github.com/sensebox/ttn-osem-integration#readme",
  "dependencies": {
    "body-parser": "^1.17.2",
    "console-control-strings": "^1.1.0",
    "express": "^4.15.3",
    "lora-serialization": "^3.0.1",
    "openSenseMapAPI": "git://github.com/sensebox/openSenseMap-API.git#357081e"
  },
  "devDependencies": {
    "chai": "^4.0.0",
    "chai-as-promised": "^6.0.0",
    "chai-things": "^0.2.0",
    "chakram": "^1.5.0",
    "eslint": "^3.19.0",
    "jsdoc": "^3.4.3",
    "mocha": "^3.4.2",
    "node-dev": "^3.1.3",
    "nyc": "^10.3.2"
  }
